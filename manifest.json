{
	"manifest_version": 2,
	"version": "1.0",
	"name": "Bias Detector",
	"description": "Used to discover political bias in news articles",
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	  },
	"browser_action": {
		"default_icon": "icon.png"
	 },
	"permissions": [
		"https://twitter.com/*"
	]
}